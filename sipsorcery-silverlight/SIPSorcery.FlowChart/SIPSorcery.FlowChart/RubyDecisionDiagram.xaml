<UserControl
	xmlns="http://schemas.microsoft.com/client/2007"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="SIPSorcery.Client.FlowChartDemo.RubyDecisionDiagram"
	d:DesignWidth="640" d:DesignHeight="480" Width="75" Height="38">

	<Canvas x:Name="m_decisionDiagramCanvas" Width="64" Height="41" MouseLeftButtonDown="Diagram_MouseDown">
		<Image Height="36.5" Width="74" Canvas.Left="-4.5" Canvas.Top="4.5" Source="Images/decisionDiagram.png"/>
		<Ellipse Height="14" Width="23" Canvas.Left="25" Canvas.Top="30.5" Fill="#FF0E2E84" Stroke="#FF000000" MouseLeftButtonDown="ConnectionSource_MouseLeftButtonDown" x:Name="m_connectionSourceBottom"/>
		<Ellipse Height="14" Width="23" Fill="#FF0E2E84" Stroke="#FF000000" Canvas.Left="24" Canvas.Top="-1.5" MouseLeftButtonUp="ConnectionSink_MouseLeftButtonUp" x:Name="m_connectionSink" MouseLeftButtonDown="ConnectionSink_MouseLeftButtonDown"/>
		<Ellipse Height="14" Width="23" Fill="#FF0E2E84" Stroke="#FF000000" MouseLeftButtonDown="ConnectionSource_MouseLeftButtonDown" x:Name="m_connectionSourceLeft" Canvas.Left="-6.5" Canvas.Top="15"/>
		<Ellipse Height="14" Width="23" Fill="#FF0E2E84" Stroke="#FF000000" MouseLeftButtonDown="ConnectionSource_MouseLeftButtonDown" x:Name="m_connectionSourceRight" Canvas.Left="51.5" Canvas.Top="17"/>
	</Canvas>
</UserControl>